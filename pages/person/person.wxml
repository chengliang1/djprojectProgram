<!--pages/person/person.wxml-->
<view class="top">
      <view class="person">
        <image class="person_icon" src="../../images/person_icon.png"></image>
        <text class="name">admin</text>
      </view>
</view>
<view class="main">
      <view class="center">
        <view class="common">
          <text class="letter">订单</text>
          <image class="arrow" src="../../images/arrow.png"></image>
        </view>
        <view class="common" >
          <text class="letter">代驾卷</text>
          <image class="arrow" src="../../images/arrow.png"></image>
        </view>
        <view class="common" bindtap="charging">
          <text class="letter">计费规则</text>
          <image class="arrow" src="../../images/arrow.png"></image>
        </view>
        <view class="common" >
          <text class="letter">法律条款</text>
          <image class="arrow" src="../../images/arrow.png"></image>
        </view>
        <view  class="common">
          <text class="letter">客服热线</text>
          <image class="arrow" src="../../images/arrow.png"></image>
          <text class="phone">4008-10-8888</text>
        </view>
      </view>
</view>
<view class="footer">
   <button bindtap="Relogin" class="saccount" type="default">切换账号</button>
</view>

